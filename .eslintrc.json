{
    "env": {
        "browser": true,
        "node": true, 
        "es2020": true
    },
    "extends": [
        "standard", 
        "eslint:recommended",
        "plugin:prettier/recommended" // prettier와 연결
		],
    "parser": "@typescript-eslint/parser",
    "parserOptions": { // 자바스크립트를 Parsing하는 옵션 설정
        "ecmaVersion": 11, // ECMA 버전
        "sourceType": "module" // import, export 사용
    },
    "plugins": [
        "@typescript-eslint", 
        "prettier"
    ],
    "rules": {
    }
}